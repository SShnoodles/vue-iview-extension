{
	"name": "vue-iview-generator",
	"displayName": "vue-iview-generator",
	"description": "generator iview vue code in VS Code",
	"version": "0.0.1",
	"author": "ssnoodles",
	"publisher": "ssnoodles",
	"license": "MIT",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vue",
		"iview"
	],
	"activationEvents": [
		"onCommand:extension.iviewForm",
		"onCommand:extension.iviewColumns",
		"onCommand:extension.iviewRules"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.iviewForm",
				"title": "Vue-iview Form"
			},
			{
				"command": "extension.iviewColumns",
				"title": "Vue-iview Columns"
			},
			{
				"command": "extension.iviewRules",
				"title": "Vue-iview Rules"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.iviewForm",
					"group": "a_iview@1"
				},
				{
					"when": "editorFocus",
					"command": "extension.iviewRules",
					"group": "a_iview@2"
				},
				{
					"when": "editorFocus",
					"command": "extension.iviewColumns",
					"group": "a_iview@3"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {}
}
